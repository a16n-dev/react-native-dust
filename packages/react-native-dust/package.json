{
  "name": "react-native-dust",
  "version": "0.1.0",
  "description": "Utility stylesheet generator for React Native with unistyles",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/a16n-dev/react-native-dust.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "bin": {
    "dust": "./dist/cli/index.js"
  },
  "exports": {
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "dev": "tsc --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@types/node": "^24.3.0",
    "commander": "^14.0.0",
    "es-toolkit": "^1.39.10",
    "fast-glob": "^3.3.3",
    "get-json-type": "^1.2.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "ts-morph": "^27.0.0",
    "chalk": "^5.6.2",
    "zod": "^4.1.11"
  },
  "optionalPeerDependencies": {
    "react-native-unistyles": "^3",
    "react-native-web": "^0.19.8"
  }
}
